<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link
      rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0/css/all.min.css"
      integrity="sha512-9usAa10IRO0HhonpyAIVpjrylPvoDwiPUiKdWk5t3PyolY1cOd4DSE0Ga+ri4AuTroPR5aQvXU9xC6qOPnzFeg=="
      crossorigin="anonymous"
      referrerpolicy="no-referrer"
    />
     <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC"
      crossorigin="anonymous"
    />
    <link rel="stylesheet" href="./css/userAllLeaveDetails.css" />
    <link rel="stylesheet" href="./css/headers.css" />
    <title>your leaves</title>
  </head>
  <body>
    <div class="container">
      {{>employeeNavbar}}
      <div class="main-container">
       <table>
          <header>
              <tr class="header_of_table">
              <th>Sr. No.</th>
              <th>Type Of Leave</th>
              <th>Shift</th>
              <th>FullDay/HalfDay</th>
              <th>1<sup>st</sup>Half / 2<sup>nd</sup>Half</th>
              <th>Dates</th>
              <th>Reason Of Leave</th>
              <th>Status</th>
            </tr>
          </header>
          <tbody class="content-div">
          </tbody>
        </table>
      </div>
      {{>paginationCard}}
    </div>
    {{>footer}}
    <script src="./js/userAllLeaveDetails.js"></script>
    <script
      src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js"
      integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM"
