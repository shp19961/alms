<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="./css/addNewUser.css" />
    <title>Login</title>
  </head>
  <body>
    <div class="container">
      <div class="form-container">
        <h1>Login</h1>
        <form class="form-data">
          <input
            id="email"
            type="email"
            class="input-group"
            placeholder="Email"
            required
          />
          <input
            id="password"
            type="password"
            class="input-group"
            placeholder="password"
            autocomplete="false"
            required
          />
          <button type="button" id="btn" class="btn" onclick="loginUser()">
            Login
          </button>
        </form>
        <p id="error"></p>
      </div>
    </div>
    <script src="./js/newUser.js"></script>
  </body>
</html>
